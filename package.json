{
  "name": "gui-for-cpu-scheduler",
  "version": "1.0.0",
  "description": "OS assignment",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . CPU --overwrite --asar=true --platform=win32 --arch=ia32 --icon=logo.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"CPU scheduler\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "author": "Abdo, Anss, Hady, Akrm",
  "license": "ISC",
  "dependencies": {
    "electron-packager": "^13.1.1",
    "chart.js": "^2.8.0",
    "electron": "^4.1.1"
  },
  "devDependencies": {
    "electron-packager": "^13.1.1",
    "chart.js": "^2.8.0",
    "electron": "^4.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AbdoWahba/CPU-Scheduler-ElectronJS-App.git"
  },
  "bugs": {
    "url": "https://github.com/AbdoWahba/CPU-Scheduler-ElectronJS-App/issues"
  },
  "homepage": "https://github.com/AbdoWahba/CPU-Scheduler-ElectronJS-App#readme"
}
